#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Github Code/Theodore/HCMUS/Phat Trien Ung Dung Web/Exercise-Github/Express-Handlebars-Exercise/node_modules/.pnpm/handlebars@4.7.8/node_modules/handlebars/bin/node_modules:/mnt/d/Github Code/Theodore/HCMUS/Phat Trien Ung Dung Web/Exercise-Github/Express-Handlebars-Exercise/node_modules/.pnpm/handlebars@4.7.8/node_modules/handlebars/node_modules:/mnt/d/Github Code/Theodore/HCMUS/Phat Trien Ung Dung Web/Exercise-Github/Express-Handlebars-Exercise/node_modules/.pnpm/handlebars@4.7.8/node_modules:/mnt/d/Github Code/Theodore/HCMUS/Phat Trien Ung Dung Web/Exercise-Github/Express-Handlebars-Exercise/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Github Code/Theodore/HCMUS/Phat Trien Ung Dung Web/Exercise-Github/Express-Handlebars-Exercise/node_modules/.pnpm/handlebars@4.7.8/node_modules/handlebars/bin/node_modules:/mnt/d/Github Code/Theodore/HCMUS/Phat Trien Ung Dung Web/Exercise-Github/Express-Handlebars-Exercise/node_modules/.pnpm/handlebars@4.7.8/node_modules/handlebars/node_modules:/mnt/d/Github Code/Theodore/HCMUS/Phat Trien Ung Dung Web/Exercise-Github/Express-Handlebars-Exercise/node_modules/.pnpm/handlebars@4.7.8/node_modules:/mnt/d/Github Code/Theodore/HCMUS/Phat Trien Ung Dung Web/Exercise-Github/Express-Handlebars-Exercise/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/handlebars" "$@"
else
  exec node  "$basedir/../../bin/handlebars" "$@"
fi
