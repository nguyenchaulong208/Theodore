<Page x:Class="BTVN08.Page2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BTVN08"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page2">

    <StackPanel>
        <TextBox x:Name="newDataTextBox" Width="200" Height="35"/>
        <Button Content="OK" Width="80" Height="35" Click="okButton_Click"/>
        <Button Content="Cancel" Width="80" Height="35" Click="cancelButton_Click"/>
    </StackPanel>
</Page>
