        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tuan_3_Example_1"
        mc:Ignorable="d"
        Title="LoginWindow" Height="226" Width="364">
    <Canvas>
        <TextBlock Text="Username" Canvas.Left="59" Canvas.Top="21"/>
        <TextBox x:Name="usernameTextBox" Canvas.Left="59" Canvas.Top="42" Height="32" Width="169"/>
        <Button Content="Login" Canvas.Left="59" Canvas.Top="79" HorizontalAlignment="Left" VerticalAlignment="Center" Width="68" Click="loginButton"/>


    </Canvas>
</Window>
